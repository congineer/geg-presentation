  "editor.minimap.enabled": false,
  // "editor.fontFamily": "Fira Code Light", // Linux
  // "editor.fontFamily": "FiraCode-Light", // macOS
  "editor.fontLigatures": true,
  "editor.fontWeight": "normal",
  "editor.fontSize": 12,
  // "editor.lineHeight": 19,
  "editor.tabSize": 2,
  "editor.renderWhitespace": "all",
  "editor.rulers": [120],
  "editor.quickSuggestions": {
    "comments": true,
    "strings": true
  },
  "editor.scrollBeyondLastLine": false,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  // Auto-formatting settings start
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "editor.formatOnType": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  // disable detect to enable auto-formatting extensions
  "editor.detectIndentation": false,
  // JS-formatting automated by Standard extention
  "standard.autoFixOnSave": true,
  // Disable default editor JS-formatting
  "javascript.validate.enable": false,
  "[javascript]": {
    "editor.formatOnType": false,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false
  },
  // Auto-formatting settings end
  "editor.renderLineHighlight": "all",
  "editor.tokenColorCustomizations": {
    "[Material Theme]": {
      // Material Design Blue Grey 300
      "comments": "#90a4ae"
    },
    "[Material Theme Ocean High Contrast]": {
      // Material Design Blue Grey 300
      "comments": "#90a4ae"
    }
  },
  // "window.zoomLevel": 0,
  // "terminal.integrated.fontSize": 12,
  "javascript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.implementationsCodeLens.enabled": true,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
